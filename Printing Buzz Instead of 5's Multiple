//Printing Buzz Instead of 5's Multiple
public class challenges {
 public static void main(String[] args) {
    int[] a = {2, 3,7,4,8};
    for (int i = 0; i < a.length - 1; i++) {
            if (a[i] + a[i + 1] == 9) {
                System.out.println("a[i]: " + a[i] + ", a[i+1]: " + a[i + 1]);
            }
    }
 }   
}
